<script>
  
  $(document).ready(function(){
    // faq content get from faq page 
    {% assign bm_faq_product_title = product.metafields.bm_pdp.bm_faqs_product_title %}
    {% comment %}
    {% assign bm_faq_product_title = 'Cranberry_PACs, Probiotic' %}
    {% endcomment %}
    {% if bm_faq_product_title contains ',' %}
      var bm_faq_product_title = '{{ bm_faq_product_title }}';
      var bm_faq_product_titles = bm_faq_product_title.split(', ');
      $.get('/pages/faq-utiva', function(data) {
        $(bm_faq_product_titles).each(function(index, value) {
          var html = $(data).find('.' + value).prop('outerHTML');
          $("#bm_faq_content").append('<div>' + html + '</div>');
          $("#bm_faq_content_mobile").append('<div>' + html + '</div>');
        });
        
        // slider for FAQ Content
        $('.bm-faq-content').slick({
          dots: true,
          arrows: true,
          infinite: true,
          speed: 400,
          slidesToShow: 1,
          adaptiveHeight: true
        });
         
//         $('.bm-faq-content .collapsible-trigger').click(function() {
//           $('.bm-faq-content').slick('refresh');
//         });

        $('#bm_faq_content_mobile .collapsible-trigger').click(function() {
          $(this).next('.collapsible-content').toggleClass('is-open bm-h-auto');
        });
        
        $('.bm-faq-content .collapsible-trigger').click(function() {
          setTimeout(function() {
            $('.bm-faq-content').slick('refresh');
          }, 100);
        });
        
      });
    {% else %}
      var faqContent = $('<div>');
      faqContent.load('/pages/faq-utiva .{{bm_faq_product_title | remove: "" }}', bm_faq_content);

      function bm_faq_content() {
        if($(window).width() < 768) {
          $( "#bm_faq_content_mobile" ).append(faqContent);
        }else{
          $( "#bm_faq_content" ).append(faqContent);
        }
      }
      $(window).resize(bm_faq_content);
    {% endif %}
  });
  
</script>