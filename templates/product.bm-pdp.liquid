        if ($(this).is(':checked')) {
          var pid = $(this).data('id');
          products.push({id: pid, quantity: 1});
        }
      });

      if (products.length > 0) {
        $.post("/cart/add.js", {items: products}).done(function() {
          window.location.href = '/cart';
        }).fail(function() {
          console.log(">> Add To Cart Error");
        })
      }
    });

