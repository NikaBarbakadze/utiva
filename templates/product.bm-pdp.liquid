      $('.bm-product-id-check').each(function() {
        if ($(this).is(':checked')) {
          var checkSaving = parseFloat($(this).data('save-price').replace('$', ''));
          price += parseFloat($(this).data('regular-price').replace('$', ''));

          if (checkSaving > 0) {
            saving += checkSaving;
          }
        }
      });

      if (price > 0) {
        $('.bm-bundle-price').text('$' + price.toFixed(2));

        if (saving > 0) {
          $('.bm-bundle-saving').text('You Save $' + saving.toFixed(2));
        } else {
          $('.bm-bundle-saving').text('');
        }
