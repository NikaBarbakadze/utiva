{% if block.settings.space_around %}
  <div class="index-section" style="background-color: #f9f9f9; margin-top: -70px;padding-top: 20px; padding-bottom: 20px;">
{% endif %}
{% if block.settings.alt %}
  <div class="index-section--alt">
{% endif %}
{% unless block.settings.full_width %}
  <div class="page-width">
{% endunless %}
  <div {{ block.shopify_attributes }}>
    <div class="rte">
      {% if block.settings.code != blank %}
        {{ block.settings.code }}
      {% else %}
        {{ 'home_page.onboarding.no_content' | t }}
      {% endif %}
    </div>
  </div>
{% unless block.settings.full_width %}
  </div>
{% endunless %}
{% if block.settings.alt %}
  </div>
{% endif %}
{% if block.settings.space_around %}
  </div>
{% endif %}
